#include<iostream>
using namespace std;
main()
	{
   
     	int n;	//negative number counter
		int num;	//input: user enters number
		int sum = 0;
		int temp;
	 //As we are dealing with the sum, so initializing with 0.
  
 //taking input from the (user)
    cout << " Start From Here : ";
    
  
do {
cin >> num;

sum = sum + num;
cout << sum << endl;


}
    //taking n numbers as input from the user and adding them to find the final sum
    while(num<n);
    
    	if(num> 0 )
	{
		
        cout << "Enter number" << num+1 << " :  ";
        num++;
    }
        cin >> temp;
        //add each number to the sum of all the previous numbers to find the final sum
        sum += temp;
        
    
    
    cout << "\nSum of the " << n << " numbers entered by the user is : "<< sum << endl;
    cout << "\n\n\n";

    return 0;
		
	}
