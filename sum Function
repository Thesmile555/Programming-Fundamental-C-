#include<iostream>
using namespace std;
//Declare Function
void PrintSum()
			{
				int a ,b;
				cout<<"Sum Function "<<endl;
				cin>>a>>b;
				cout<<"sum is "<<a+b<<endl;
	
			}
void Printsubt()
			{
				int a ,b;
				cout<<"subtraction Function "<<endl;
				cin>>a>>b;
				cout<<"subtravtion is "<<a-b<<endl;
	
			}
void Printmult()
			{
				int a ,b;
				cout<<"Multiplication Function "<<endl;
				cin>>a>>b;
				cout<<"Multiplication  is "<<a*b<<endl;
	
			}
void Printdivide()
			{
				int a ,b;
				cout<<"Divide Function "<<endl;
				cin>>a>>b;
				cout<<"Division  is "<<a/b<<endl;
	
			}
		

	main()
			{
				//Calling Function
				
				 PrintSum();
				 Printsubt();
				 Printmult();
				 Printdivide();
				
				 
			}
