#include<iostream>
using namespace std;
main()
	{
		int nagitive_number;
		int number;
		int sum = 0; 
		cout<<"Start From Here --> ";
		
		do
			{
				cin>>number;
				sum = sum + number;
				cout<<sum<<endl;
				
			}
			 while(number<nagitive_number || number > nagitive_number );
		
				cout<<number;
				sum++;
			
				}
